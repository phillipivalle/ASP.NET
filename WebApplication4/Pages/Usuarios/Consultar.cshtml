@page
@model WebApplication4.Pages.Usuarios.ConsultarModel
@{
    ViewData["Title"] = "Consultar";
}

<h2>Consultar</h2>


<form method="post">
    <div class="form-group">
        <label>CPF</label>
        <input type="text" name="cpf" />
     </div>
    <button> Buscar </button>

</form>
@if(ViewData["usuario"] != null){
    <div class="card">
        @(((WebApplication4.Data.Usuario)ViewData["usuario"]).Nome)
    </div>
}